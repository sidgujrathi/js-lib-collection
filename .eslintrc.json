{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style": "off",
        "no-extra-semi": "off",
        "no-console": "off",
        "class-methods-use-this": "off",
        "import/no-unresolved": "off", //This is just because this is just collection
        "no-underscore-dangle": "off",
        "max-len": [
            "error",
            {
                "code": 220
            }
        ]
    }
}